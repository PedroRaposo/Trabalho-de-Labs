import xlsxwriter

workbook = xlsxwriter.Workbook('tabelas_iniciais.xlsx')
Gene_Identification = workbook.add_worksheet('Gene_Identification')

Gene_Identification_1 = (
    []
)

row = 1
col = 0

for GeneID, accession_number, locus, gene_name, strand in (Gene_Identification_1):
    Gene_Identification.write(row, col, GeneID)
    Gene_Identification.write(row, col +1, accession_number)
    Gene_Identification.write(row, col +2, locus)
    Gene_Identification.write(row, col +3, gene_name)
    Gene_Identification.write(row, col +4, strand)
    row += 1

Gene_Identification.write(0, 0, 'GeneID')
Gene_Identification.write(0, 1, 'accession_number')
Gene_Identification.write(0, 2, 'locus')
Gene_Identification.write(0, 3, 'gene_name')
Gene_Identification.write(0, 4, 'strand') 

workbook.close()
