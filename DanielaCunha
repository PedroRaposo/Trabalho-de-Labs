@author: Daniela
"""

import xlsxwriter

workbook = xlsxwriter.Workbook('tabela_inicial.xlsx')
Gene_Anotation = workbook.add_worksheet()

Gene_Anotation_1 = (
    []
)

row = 2
col = 0

for Gene_Identification, GeneID_NCBI, Accession_Number_NCBI, Locus_Tag, Gene_Name, Strand, DNA_Sequence, Protein_Identification, UniprotID, Curation_Level, Accession_Protein_Number_NCBI, Protein_Name, Protein_Properties, Number_Aminoacids, Aminoacid_sequence, \
Cellular_Location, Gene_Ontology, EC_or_TC_numbers, Description, Comments in (Gene_Anotation_1):
    
    Gene_Anotation.write(row, col, Gene_Identification)
    Gene_Anotation.write(row +1, col +1, GeneID_NCBI)
    Gene_Anotation.write(row +1, col +2, Accession_Number_NCBI)
    Gene_Anotation.write(row +1, col +3, Locus_Tag)
    Gene_Anotation.write(row +1, col +4, Gene_Name)
    Gene_Anotation.write(row +1, col +4, Strand)
    Gene_Anotation.write(row +1, col +5, DNA_Sequence)
    
    Gene_Anotation.write(row, col +6, Protein_Identification)
    Gene_Anotation.write(row +1, col +6, UniprotID)
    Gene_Anotation.write(row +1, col +7, Curation_Level)
    Gene_Anotation.write(row +1, col +8, Accession_Protein_Number_NCBI)
    Gene_Anotation.write(row +1, col +9, Protein_Name)
    Gene_Anotation.write(row +1, col +10, Protein_Properties)
    Gene_Anotation.write(row +1, col +11, Number_Aminoacids)
    Gene_Anotation.write(row +1, col +12, Aminoacid_sequence)
    
    Gene_Anotation.write(row +1, col +13, Cellular_Location)
    Gene_Anotation.write(row +1, col +14, Gene_Ontology)
    Gene_Anotation.write(row +1, col +15, EC_or_TC_numbers)
    Gene_Anotation.write(row +1, col +16, Description)
    Gene_Anotation.write(row +1, col +17, Comments)
   
    row += 1

Gene_Anotation.write(0, 0, 'Gene_Identification')
Gene_Anotation.write(1, 0, 'GeneID_NCBI')
Gene_Anotation.write(1, 1, 'Accession_Number_NCBI')
Gene_Anotation.write(1, 2, 'Locus_Tag')
Gene_Anotation.write(1, 3, 'Gene_Name') 
Gene_Anotation.write(1, 4, 'Strand') 
Gene_Anotation.write(1, 5, 'DNA_Sequence') 

Gene_Anotation.write(0, 6, 'Protein_Identification')
Gene_Anotation.write(1, 6, 'UniprotID')
Gene_Anotation.write(1, 7, 'Curation_Level')
Gene_Anotation.write(1, 8, 'Accession_Protein_Number_NCBI')
Gene_Anotation.write(1, 9, 'Protein_Name') 
Gene_Anotation.write(1, 10, 'Protein_Properties') 
Gene_Anotation.write(1, 11, 'Number_Aminoacids')
Gene_Anotation.write(1, 12, 'Aminoacid_sequence')

Gene_Anotation.write(0, 13, 'Cellular_Location')
Gene_Anotation.write(0, 14, 'Gene_Ontology')
Gene_Anotation.write(0, 15, 'EC_or_TC_numbers')
Gene_Anotation.write(0, 16, 'Description')
Gene_Anotation.write(0, 17, 'Comments')

workbook.close()
